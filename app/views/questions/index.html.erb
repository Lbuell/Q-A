  <!-- Main Feed -->
  <!-- This has been source ordered to come first in the markup (and on small devices) but to be to the right of the nav on larger screens -->
  <div class="large-6 columns">
    <% @questions.each do |question| %>
    <tr>
      <!-- Feed Entry -->
      <div class="row">
        <div class="large-2 columns small-3"><img src="../assets/6.jpg" alt=""></div>
        <div class="large-10 columns">
          <p>
            <ul>
            </ul>
          </p>

          <td><% question.users.each do |user| %></td>
          <p><td><%= link_to user.email, user %> Says: </td></p>
          <p><strong>Question:</strong>
            <td><%= question.question %></td>

            <p>
              <strong>Answer:</strong>

              <td><%= question.answer %></td>
              <% end %>
            </p>
            <ul class="inline-list">
              <li><%= link_to 'Reply', edit_question_path(question) %></li>
            </ul>
            <p>
              <b>Peeps who have contributed:</b>

              <td><% @questions.each do |question| %></td>
              <td><% question.users.each do |user| %></td>
              <p><td><%= link_to user.email, user %></td></p>
                <% end %>
              <% end %>

              <td><%= link_to 'Destroy', question, method: :delete, data: { confirm: 'Are you sure?' } %></td></p>
            </tr>
            <% end %>


          </div>
        </div>


        <hr />

        <!-- Feed Entry -->
        <div class="row">
          <div class="large-2 columns small-3"><img src="../assets/7.jpg" alt=""></div>
          <div class="large-10 columns">
